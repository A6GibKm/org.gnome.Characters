        "*.a",
        "/share/doc",
        "/share/gir-1.0"
    ],
    "modules" : [
        {
            "name" : "gnome-desktop",
            "buildsystem" : "meson",
            "config-opts" : [
                "-Ddebug-tools=false",
                "-Dudev=disabled"
            ],
            "sources" : [
                {
                    "type" : "archive",
